{
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "space-before-function-paren": false,
        "trailing-comma": [true, { "multiline": "always" }],
        "quotemark": [true, "single", "jsx-double"],
        "no-var-requires": false
    }
}
